{
  "name": "ClearCue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate:icons": "node scripts/generate-icons.js && node scripts/generate-android-icons.js",
    "generate:ios-icons": "node scripts/generate-icons.js",
    "generate:android-icons": "node scripts/generate-android-icons.js",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ClearCue2.xcworkspace -scheme ClearCue -configuration Release -destination generic/platform=iOS -archivePath ClearCue.xcarchive archive",
    "build:ios:simulator": "cd ios && xcodebuild -workspace ClearCue2.xcworkspace -scheme ClearCue -configuration Release -destination 'platform=iOS Simulator,name=iPhone 15' build",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean -workspace ClearCue2.xcworkspace -scheme ClearCue",
    "clean": "npm run clean:android && npm run clean:ios",
    "pod:install": "cd ios && pod install",
    "pod:update": "cd ios && pod update",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run lint && npm run typecheck",
    "build:production": "./scripts/build-production.sh",
    "build:production:android": "./scripts/build-production.sh android",
    "build:production:ios": "./scripts/build-production.sh ios",
    "validate:translations": "node scripts/validate-translations.js",
    "fix:translations": "node scripts/fix-translations.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-firebase/app": "^20.5.0",
    "@react-native-firebase/auth": "^20.5.0",
    "@react-native-firebase/firestore": "^20.5.0",
    "@react-native-firebase/messaging": "^20.5.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/material-top-tabs": "^7.3.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@types/react-native-permissions": "^1.1.1",
    "@types/react-native-push-notification": "^8.1.4",
    "date-fns": "^4.1.0",
    "i18next": "^25.2.1",
    "lucide-react-native": "^0.523.0",
    "luxon": "^3.6.1",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-i18next": "^15.5.3",
    "react-native": "0.75.5",
    "react-native-background-job": "^2.3.1",
    "react-native-big-calendar": "^4.18.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-date-picker": "^5.0.13",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-pager-view": "^6.8.1",
    "react-native-permissions": "^5.4.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.37.0",
    "react-native-svg": "^15.12.0",
    "react-native-tab-view": "^4.1.2",
    "rrule": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/babel-preset": "0.75.5",
    "@react-native/eslint-config": "0.75.5",
    "@react-native/metro-config": "0.75.5",
    "@react-native/typescript-config": "0.75.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.6.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "sharp": "^0.34.2",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
